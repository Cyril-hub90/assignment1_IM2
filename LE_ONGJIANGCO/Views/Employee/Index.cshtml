@model System.Data.DataTable

@{
    ViewData["Title"] = "Employee List";
}

<h2>Employee List</h2>

<a asp-action="Register">Register New Employee</a> |
<a asp-action="Login">Logout</a>

<table border="1" cellpadding="5" cellspacing="0" width="100%">
    <tr>
        <th>Full Name</th>
        <th>Address</th>
        <th>Contact</th>
        <th>Email</th>

    </tr>
    @foreach (System.Data.DataRow row in Model.Rows)
    {
        <tr>
            <td>@row["emp_fullname"]</td>
            <td>@row["emp_addr"]</td>
            <td>@row["emp_contact"]</td>
            <td>@row["emp_email"]</td>

        </tr>
    }
</table>
